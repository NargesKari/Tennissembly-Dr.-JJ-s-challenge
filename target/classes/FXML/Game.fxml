   <VBox fx:id="vbox" alignment="CENTER" prefHeight="168.0" prefWidth="168.0">
      <children>
         <Label alignment="CENTER" prefHeight="17.0" prefWidth="169.0" text="Chose movement">
            <font>
               <Font name="Comic Sans MS Bold" size="15.0" />
            </font></Label>
         <ChoiceBox fx:id="choiceBox" depthTest="DISABLE" prefHeight="25.0" prefWidth="172.0">
            <cursor>
               <Cursor fx:constant="DEFAULT" />
            </cursor></ChoiceBox>
         <HBox prefHeight="39.0" prefWidth="168.0">
            <children>
               <Label text=" a:">
                  <font>
                     <Font name="Comic Sans MS Bold" size="13.0" />
                  </font></Label>
               <TextField fx:id="a" prefHeight="25.0" prefWidth="30.0" />
               <Label text=" b:">
                  <font>
                     <Font name="Comic Sans MS Bold" size="13.0" />
                  </font></Label>
               <TextField fx:id="b" prefHeight="25.0" prefWidth="30.0" />
               <Label text=" c:">
                  <font>
                     <Font name="Comic Sans MS Bold" size="13.0" />
                  </font></Label>
               <TextField fx:id="c" prefHeight="25.0" prefWidth="30.0" />
            </children>
         </HBox>
         <Button mnemonicParsing="false" onMouseClicked="#applyMovement" prefHeight="50.0" prefWidth="80.0" text="APPLY" />
      </children>
      <effect>
         <DropShadow height="213.05" radius="99.0275" spread="0.8" width="185.06">
